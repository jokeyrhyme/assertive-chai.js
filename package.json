{
  "name": "assertive-chai",
  "version": "2.0.0",
  "description": "Chai.js without Expect or Should",
  "main": "assertive-chai.js",
  "scripts": {
    "build": "$(npm bin)/gulp build:lib",
    "test": "$(npm bin)/gulp test:nodejs test:phantomjs"
  },
  "keywords": [
    "test",
    "assertion",
    "assert",
    "testing",
    "chai"
  ],
  "repository": "https://github.com/jokeyrhyme/assertive-chai.js.git",
  "author": "\"Ron Waldon\" (http://jokeyrhy.me/)",
  "license": "MIT",
  "devDependencies": {
    "babelify": "^5.0.4",
    "backbone": "^1.1.2",
    "browserify": "^9.0.3",
    "es5-shim": "^4.1.0",
    "gulp": "^3.8.11",
    "gulp-mocha": "^2.0.0",
    "gulp-mocha-phantomjs": "^0.5.3",
    "json3": "^3.3.2",
    "mocha": "^2.2.1",
    "requirejs": "^2.1.16",
    "vinyl-transform": "^1.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "whitelist": [
            "es3.memberExpressionLiterals",
            "es3.propertyLiterals"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "deep-eql": "0.1.3",
    "pathval": "0.1.1",
    "type-detect": "chaijs/type-detect#1.0.0"
  }
}
